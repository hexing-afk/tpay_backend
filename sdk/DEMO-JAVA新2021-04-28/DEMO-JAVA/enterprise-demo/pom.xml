          <scope>system</scope>
          <systemPath>${project.basedir}/lib/top-commons.jar
          </systemPath>
      </dependency>
      <dependency>
          <groupId>top-certapi</groupId>
          <artifactId>top-certapi</artifactId>
          <version>1.0</version>
          <scope>system</scope>
          <systemPath>${project.basedir}/lib/top-certapi.jar
          </systemPath>
      </dependency>

      <dependency>
          <groupId>bcmail-jdk15</groupId>
